msgid "[[!meta title=\"Numerous security holes in Tails 3.6\"]]\n"
msgstr "[[!meta title=\"Plusieurs failles de sécurité dans Tails 3.6\"]]\n"

#. type: Plain text
#, no-wrap
msgid "[[!tag security/fixed]]\n"
